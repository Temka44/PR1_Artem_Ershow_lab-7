		<xs:element name="FLOWER" minOccurs="0" maxOccurs="unbounded">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Sort_Flower" type="xs:string"/>
				<xs:element name="Name_Flower" type="xs:string"/>
				<xs:element name="Length_Flower" type="xs:string"/>
				<xs:element name="Type_Flower" type="xs:string"/>
				<xs:element name="About_Flower" type="xs:string"/>			
			</xs:sequence>
			<xs:attribute name="IDFlower" type="xs:integer"/>
		</xs:complexType>
		</xs:element>
			</xs:choice>  
		</xs:complexType>
